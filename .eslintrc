{
  "env": {
    "browser": true,
    "es2021": true
  },
    "extends": [
      "plugin:react/recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "react/no-find-dom-node": 0
    }
}
